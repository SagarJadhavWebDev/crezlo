{
  "name": "crezlo-core",
  "version": "2.0.0",
  "type": "module",
  "description": "A comprehensive React component library with TypeScript, Tailwind CSS, and theme support for reusable UI components",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./components": {
      "import": "./dist/components/index.esm.js",
      "require": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./hooks": {
      "import": "./dist/hooks/index.esm.js",
      "require": "./dist/hooks/index.js",
      "types": "./dist/hooks/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.esm.js",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    },
    "./context-providers": {
      "import": "./dist/context-providers/index.esm.js",
      "require": "./dist/context-providers/index.js",
      "types": "./dist/context-providers/index.d.ts"
    },
    "./constants": {
      "import": "./dist/constants/index.esm.js",
      "require": "./dist/constants/index.js",
      "types": "./dist/constants/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.esm.js",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./styles.css": {
      "default": "./dist/index.css"
    }
  },
  "typesVersions": {
    "*": {
      "components": [
        "./dist/components/index.d.ts"
      ],
      "hooks": [
        "./dist/hooks/index.d.ts"
      ],
      "utils": [
        "./dist/utils/index.d.ts"
      ],
      "constants": [
        "./dist/constants/index.d.ts"
      ],
      "types": [
        "./dist/types/index.d.ts"
      ],
      "context-providers": [
        "./dist/context-providers/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles/index.css -o ./dist/index.css && tsc -p tsconfig.json && rollup -c ",
    "dev": "concurrently \"tailwindcss -i ./src/styles/index.css -o ./dist/index.css --watch\" \"tsc -p tsconfig.json --watch\" \"rollup -c -w\" ",
    "dev:rollup": "rollup -c -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "react",
    "typescript",
    "tailwindcss",
    "components",
    "theme",
    "ui-library",
    "reusable-components"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SagarJadhavWebDev/crezlo.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^26.0.3",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/lodash": "^4.17.20",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.17",
    "@types/react-day-picker": "^5.3.0",
    "@types/react-dom": "^18.3.5",
    "@types/recharts": "^2.0.1",
    "autoprefixer": "^10.4.20",
    "lodash": "^4.17.21",
    "postcss": "^8.5.6",
    "rollup": "^4.28.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-preserve-use-client": "^3.0.1",
    "tailwindcss": "^4.1.11",
    "tslib": "^2.8.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@rollup/plugin-alias": "^5.1.1",
    "@tailwindcss/cli": "^4.1.11",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "concurrently": "^9.2.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "lucide-react": "^0.525.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.2",
    "react-day-picker": "^9.8.0",
    "react-hook-form": "^7.60.0",
    "react-resizable-panels": "^3.0.3",
    "recharts": "^3.1.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "sonner": "^2.0.6",
    "tailwind-merge": "^3.3.1",
    "vaul": "^1.1.2"
  }
}
